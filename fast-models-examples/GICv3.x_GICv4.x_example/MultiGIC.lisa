    // Control port behavior for gic.pvbus_s address mapping
    internal slave port<PVBusMapperControl> map_control
    {
        slave behavior remap( pv::RemapRequest& req_ ): unsigned
        {
            // Get the original address.
            const pv::bus_addr_t orig = req_.getOriginalTransactionAddress();
            // Base address of GICs
            const pv::bus_addr_t base_addr = reg_base;

            if (orig >= base_addr && orig < ((pv::bus_addr_t) reg_base + gic_offset))
            {
                return 0; // Gic 0 address space
            }
            else if (orig >= base_addr + gic_offset && orig < ((pv::bus_addr_t) reg_base + 2 * gic_offset))
            {
                return 1; // Gic 1 addres space
            }
            else if (orig >= base_addr + 2 * gic_offset && orig < ((pv::bus_addr_t) reg_base + 3 * gic_offset))
            {
                return 2; // Gic 2 addres space
            }
            else if (orig >= base_addr + 3 * gic_offset && orig < ((pv::bus_addr_t) reg_base + 4 * gic_offset))
            {
                return 3; // Gic 3 addres space
            }
            else if (orig >= base_addr + 4 * gic_offset && orig < ((pv::bus_addr_t) reg_base + 5 * gic_offset))
            {
                return 4; // Gic 4 addres space
            }
            else if (orig >= base_addr + 5 * gic_offset && orig < ((pv::bus_addr_t) reg_base + 6 * gic_offset))
            {
                return 5; // Gic 5 addres space
            }
            else if (orig >= base_addr + 6 * gic_offset && orig < ((pv::bus_addr_t) reg_base + 7 * gic_offset))
            {
                return 6; // Gic 6 addres space
            }
            else if (orig >= base_addr + 7 * gic_offset && orig < ((pv::bus_addr_t) reg_base + 8 * gic_offset))
            {
                return 7; // Gic 7 addres space
            }
            else if (orig >= base_addr + 8 * gic_offset && orig < ((pv::bus_addr_t) reg_base + 9 * gic_offset))
            {
                return 8; // Gic 8 addres space
            }
            else if (orig >= base_addr + 9 * gic_offset && orig < ((pv::bus_addr_t) reg_base + 10 * gic_offset))
            {
                return 9; // Gic 9 addres space
            }
            else if (orig >= base_addr + 10 * gic_offset && orig < ((pv::bus_addr_t) reg_base + 11 * gic_offset))
            {
                return 10; // Gic 10 addres space
            }
            else if (orig >= base_addr + 11 * gic_offset && orig < ((pv::bus_addr_t) reg_base + 12 * gic_offset))
            {
                return 11; // Gic 11 addres space
            }
            else if (orig >= base_addr + 12 * gic_offset && orig < ((pv::bus_addr_t) reg_base + 13 * gic_offset))
            {
                return 12; // Gic 12 addres space
            }
            else if (orig >= base_addr + 13 * gic_offset && orig < ((pv::bus_addr_t) reg_base + 14 * gic_offset))
            {
                return 13; // Gic 13 addres space
            }
            else if (orig >= base_addr + 14 * gic_offset && orig < ((pv::bus_addr_t) reg_base + 15 * gic_offset))
            {
                return 14; // Gic 14 addres space
            }
            else if (orig >= base_addr + 15 * gic_offset && orig < ((pv::bus_addr_t) reg_base + 16 * gic_offset))
            {
                return 15; // Gic 15 addres space
            }
            else {
                return 0;
            }
        }
    }

